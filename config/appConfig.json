{
  "directories": {
    "appDir": "/app",
    "dataDir": "/data",
    "publicDir": "/public",
    "privateDir": "/private"
  },
  "app": {
    "name": "Catch o the Day",
    "rootPage": "app.js",
    "dev": "_gen_dev",
